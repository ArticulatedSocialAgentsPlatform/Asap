<?xml version="1.0" standalone="no"?>
<!DOCTYPE definition SYSTEM "./utterances.dtd">

<definition>
    <utterance>
        <behaviorspec id="gesture_part1">
            <gesture id="gebiss" scope="hand">
                <timing onset="1.0" end="3.0"/>
                <constraints>
                    <sequence>
                        <parallel>
                            <static scope="right_arm" slot="PalmOrientation" value="DirD"/>
                            <static scope="right_arm" slot="ExtFingerOrientation" value="DirA"/>
                            <static scope="right_arm" slot="HandShape" value="ASLclarge"/>
                            <dynamic scope="right_arm" slot="HandLocation">
                                <dynamicElement scope="right_arm" type="linear" >
                                    <value type="start" name="LocStomach LocExtremePeripheryRight LocNorm"/>
                                    <value type="end" name="LocHip LocExtremePeripheryRight LocNorm"/>
                                </dynamicElement>
                            </dynamic>
                        </parallel>
                        <parallel>
                            <static scope="right_arm" slot="PalmOrientation" value="DirT"/>
                            <static scope="right_arm" slot="ExtFingerOrientation" value="DirL"/>
                            <dynamic scope="right_arm" slot="HandLocation">
                                <dynamicElement scope="right_arm" type="linear" >
                                    <value type="start" name="LocHip LocExtremePeripheryRight LocNorm"/>
                                    <value type="end" name="LocNose LocCCenterRight LocNorm"/>
                                </dynamicElement>
                            </dynamic>
                        </parallel>
                        <parallel>
                            <static scope="right_arm" slot="PalmOrientation" value="DirT"/>
                            <static scope="right_arm" slot="ExtFingerOrientation" value="DirL"/>
                            <dynamic scope="right_arm" slot="HandLocation">
                                <dynamicElement scope="right_arm" type="linear" >
                                    <value type="start" name="LocNose LocCCenterRight LocNorm"/>
                                    <value type="end" name="LocNose LocCCenterRight LocNear"/>
                                </dynamicElement>
                            </dynamic>
                        </parallel>
                    </sequence>
                </constraints>
            </gesture>
        </behaviorspec>
        <behaviorspec id="gesture_part2">
            <gesture id="gebiss" scope="hand">
                <timing onset="1.0" end="2.5"/>
                <constraints>
                    <static slot="GazeDirection" value="1000 0 0" scope="head"/>
                </constraints>
            </gesture>
        </behaviorspec>
        <behaviorspec id="gesture_part3">
            <gesture id="gebiss" scope="hand">
                <timing onset="3.0" end="5.0"/>
                <constraints>
                    <static slot="GazeDirection" value="50 0 50" scope="head"/>
                </constraints>
            </gesture>
        </behaviorspec>
    </utterance>
</definition>