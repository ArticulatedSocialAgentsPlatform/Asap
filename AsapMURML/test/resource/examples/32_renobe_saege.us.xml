<?xml version="1.0"?>
<bml xmlns="http://www.bml-initiative.org/bml/bml-1.0" id="bml1">
    
        
            <murml-gesture xmlns="http://www.techfak.uni-bielefeld.de/ags/soa/murml"><murml-description scope="hand"><parallel>
                        <static scope="right_arm" slot="PalmOrientation" value="DirTL"/>
                        <static scope="right_arm" slot="ExtFingerOrientation" value="DirA"/>
                        <static scope="left_arm" slot="PalmOrientation" value="DirD"/>
                        <static scope="left_arm" slot="ExtFingerOrientation" value="DirA"/>
                        <static scope="left_arm" slot="HandLocation" value="LocStomach LocPeripheryLeft LocNorm"/>
                        <dynamic scope="right_arm" slot="HandLocation">
                            <dynamicElement scope="right_arm" type="linear">
                                <value type="start" name="LocLowerChest LocCenterLeft LocNear"/>
                                <value type="end" name="LocStomach LocLeft LocStreched"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear">
                                <value type="start" name="LocStomach LocLeft LocStreched"/>
                                <value type="end" name="LocLowerChest LocCenterLeft LocNear"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear">
                                <value type="start" name="LocLowerChest LocCenterLeft LocNear"/>
                                <value type="end" name="LocStomach LocLeft LocStreched"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear">
                                <value type="start" name="LocStomach LocLeft LocStreched"/>
                                <value type="end" name="LocLowerChest LocCenterLeft LocNear"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear">
                                <value type="start" name="LocLowerChest LocCenterLeft LocNear"/>
                                <value type="end" name="LocStomach LocLeft LocStreched"/>
                            </dynamicElement>
                        </dynamic>
                    </parallel></murml-description></murml-gesture>
        
        
            <murml-gesture xmlns="http://www.techfak.uni-bielefeld.de/ags/soa/murml"><murml-description scope="hand"><static slot="GazeDirection" value="1000 500 0" scope="head"/></murml-description></murml-gesture>
        
        
            <murml-gesture xmlns="http://www.techfak.uni-bielefeld.de/ags/soa/murml"><murml-description scope="hand"><static slot="GazeDirection" value="50 0 50" scope="head"/></murml-description></murml-gesture>
        
    
</bml>
