<?xml version="1.0" standalone="no"?>
<!DOCTYPE definition SYSTEM "./utterances.dtd">

<definition>
    <utterance>
        <behaviorspec id="gesture_part1">
            <gesture id="saege" scope="hand">
                <timing onset="1.0" end="3.0"/>
                <constraints>
                    <parallel>
                        <static scope="right_arm" slot="PalmOrientation" value="DirTL"/>
                        <static scope="right_arm" slot="ExtFingerOrientation" value="DirA"/>
                        <static scope="left_arm" slot="PalmOrientation" value="DirD"/>
                        <static scope="left_arm" slot="ExtFingerOrientation" value="DirA"/>
                        <static scope="left_arm" slot="HandLocation" value="LocStomach LocPeripheryLeft LocNorm"/>
                        <dynamic scope="right_arm" slot="HandLocation">
                            <dynamicElement scope="right_arm" type="linear" >
                                <value type="start" name="LocLowerChest LocCenterLeft LocNear"/>
                                <value type="end" name="LocStomach LocLeft LocStreched"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear" >
                                <value type="start" name="LocStomach LocLeft LocStreched"/>
                                <value type="end" name="LocLowerChest LocCenterLeft LocNear"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear" >
                                <value type="start" name="LocLowerChest LocCenterLeft LocNear"/>
                                <value type="end" name="LocStomach LocLeft LocStreched"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear" >
                                <value type="start" name="LocStomach LocLeft LocStreched"/>
                                <value type="end" name="LocLowerChest LocCenterLeft LocNear"/>
                            </dynamicElement>
                            <dynamicElement scope="right_arm" type="linear" >
                                <value type="start" name="LocLowerChest LocCenterLeft LocNear"/>
                                <value type="end" name="LocStomach LocLeft LocStreched"/>
                            </dynamicElement>
                        </dynamic>
                    </parallel>
                </constraints>
            </gesture>
        </behaviorspec>
        <behaviorspec id="gesture_part2">
            <gesture id="saege" scope="hand">
                <timing onset="1.0" end="3.0"/>
                <constraints>
                    <static slot="GazeDirection" value="1000 500 0" scope="head"/>
                </constraints>
            </gesture>
        </behaviorspec>
        <behaviorspec id="gesture_part3">
            <gesture id="saege" scope="hand">
                <timing onset="4.0" end="5.0"/>
                <constraints>
                    <static slot="GazeDirection" value="50 0 50" scope="head"/>
                </constraints>
            </gesture>
        </behaviorspec>
    </utterance>
</definition>