<configuration debug="true">
	<appender name="STDOUT"
    class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        %d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n
     </pattern>
    </encoder>
  </appender>


  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>  
  <logger name="hmi.elckerlyc.parametervaluechange.TimedParameterValueChangeUnit" level="DEBUG">
    <appender-ref ref="STDOUT" />
  </logger>  
</configuration>