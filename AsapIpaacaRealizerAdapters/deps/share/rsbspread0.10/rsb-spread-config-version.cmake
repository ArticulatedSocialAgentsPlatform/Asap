SET(RSBSPREAD_RSBSPREAD_PACKAGE_VERSION "0.10.0")

# Check whether the requested PACKAGE_FIND_VERSION is compatible
IF("${RSBSPREAD_PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}")
    SET(RSBSPREAD_PACKAGE_VERSION_COMPATIBLE FALSE)
ELSE()
    SET(RSBSPREAD_PACKAGE_VERSION_COMPATIBLE TRUE)
    IF("${RSBSPREAD_PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}")
        SET(RSBSPREAD_PACKAGE_VERSION_EXACT TRUE)
    ENDIF()
ENDIF()
    
