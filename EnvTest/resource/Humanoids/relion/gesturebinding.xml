			<constraint name="hand" value="RIGHT"/>	
		</constraints>
		<parametermap>			
					
		</parametermap>
		<MotionUnit type="ProcAnimation" file="Humanoids/shared/procanimation/conductor/3-beat_pattern_RH.xml"/>
	</MotionUnitSpec>
	
	<MotionUnitSpec type="gesture">
		<constraints>		
			<constraint name="type" value="LEXICALIZED"/>			
			<constraint name="lexeme" value="conduct-3-beat"/>			
			<constraint name="hand" value="LEFT"/>	
		</constraints>
		<parameterdefaults>
			<parameterdefault name="mirror" value="true"/>
			<parameterdefault name="joints" value="l_shoulder"/>
		</parameterdefaults>
		<parametermap>			
					
		</parametermap>
		<MotionUnit type="ProcAnimation" file="Humanoids/shared/procanimation/conductor/3-beat_pattern_RH.xml"/>
	</MotionUnitSpec>

		<MotionUnitSpec type="noise" namespace="http://hmi.ewi.utwente.nl/bmlt">
		<constraints>
			<constraint name="type" value="perlin"/>						
		</constraints>
		<parametermap>
			<parameter src="joint" dst="joint"/>			
			<parameter src="baseamplitudex" dst="baseamplitudex"/>			
			<parameter src="baseamplitudey" dst="baseamplitudey"/>			
			<parameter src="baseamplitudez" dst="baseamplitudez"/>			
			<parameter src="offsetx" dst="offsetx"/>			
			<parameter src="offsety" dst="offsety"/>			
			<parameter src="offsetz" dst="offsetz"/>			
			<parameter src="basefreqx" dst="basefreqx"/>			
			<parameter src="basefreqy" dst="basefreqy"/>			
			<parameter src="basefreqz" dst="basefreqz"/>			
			<parameter src="persistencex" dst="persistencex"/>			
			<parameter src="persistencey" dst="persistencey"/>			
			<parameter src="persistencez" dst="persistencez"/>			
		</parametermap>
		<MotionUnit type="class" class="hmi.animationengine.noise.PerlinNoiseMU"/>
	</MotionUnitSpec>

